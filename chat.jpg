                $(this).parent("li").removeClass("active").addClass("inactive");
                 $(this).parent("li").children("ul.level-1").slideUp(); 
            }
            else{
                $("ul.level-1").slideUp();
                  $("ul.cat-menu > li").removeClass("active");
                  $(this).parent("li").removeClass("inactive").addClass("active");
                  $(this).parent("li").children("ul.level-1").slideDown();      
            }
        }
        else{
            if($(this).parent("li").parent("ul").hasClass("level-1")){
                 if($(this).parent("li").hasClass("active")){  
                        $(this).parent("li").removeClass("active").addClass("inactive");
                        $(this).parent("li").children("ul.level-2").slideUp();   
                }
                else{
                      $("ul.level-2").slideUp();
                      $("ul.level-1 > li").removeClass("active");
                      $(this).parent("li").removeClass("inactive").addClass("active");
                      $(this).parent("li").children("ul.level-2").slideDown();       
                } 
            }
        }
    });    
          });
